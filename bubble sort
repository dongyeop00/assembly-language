MAIN	SEGMENT
	    ASSUME	CS:MAIN	DS:MAIN

	      MOV	AX,CS
	      MOV	DS,AX

        MOV	SI,0
	      MOV	DI,0
	      MOV	CX,0

LOOP1:	CMP	CX,5
	      JAE	ELOOP
	      MOV	SI,0
	      MOV	DI,2
	      INC	CX
	
LOOP3:	MOV	AX,DATA1[SI]
	      CMP	AX,DATA1[DI]
	      JA	LOOP2
	      CMP	DI,8
	      JAE	LOOP1
	      ADD	SI,2
	      ADD	DI,2
	      JMP	LOOP3

LOOP2:	MOV	BX,DATA1[DI]
	      MOV	DATA1[SI],BX
	      MOV	DATA1[DI],AX
	      JMP	LOOP3

ELOOP:	MOV	AH,4CH
	      INT	21H


DATA1	DW	35,26,20,3,100


MAIN	ENDS
	    END
